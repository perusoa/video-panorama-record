yer__video {\n  position: relative;\n  padding-bottom: 56.25%;\n}\n\n.c-video-player__video--panorama {\n  padding-bottom: 23.4375%;\n}\n\n.c-video-player__player,\n.c-video-player__canvas,\n.canvas-container {\n  position: absolute !important;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n}\n\n.c-video-player-mode {\n  display: flex;\n  justify-content: center;\n  width: 100%;\n  padding: 20px;\n  background-color: rgba(0, 0, 0, 0.04);\n}\n\n.c-video-player-controls {\n  z-index: 20;\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  width: 100%;\n  background-image: linear-gradient(transparent, rgba(0, 0, 0, 0.9));\n}\n\n.c-video-player-controls__wrapper {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  width: 100%;\n  padding: 10px;\n}\n\n.c-video-player-controls__seeks {\n  margin-left: 5px;\n  margin-right: 20px;\n}\n\n@keyframes fade {\n  0%,100% { opacity: 0.5 }\n  50% { opacity: 1 }\n}\n\n.c-video-player-controls__record {\n  cursor: pointer;\n  appearance: none;\n  background: none;\n  border: none;\n  display: flex;\n  align-items: center;\n  color: red;\n  text-transform: uppercase;\n  font-weight: 600;\n  font-size: 12px;\n  margin-right: 10px;\n  opacity: 1;\n}\n\n.c-video-player-controls__record.c-video-player-controls__record--recording {\n  animation: fade 2s linear infinite;\n}\n\n.c-video-player-controls__record i {\n  font-size: 18px;\n  margin-left: 4px;\n}\n\n.c-video-player-controls__play-pause,\n.c-video-player-controls__fullscreen,\n.c-video-player-controls__seek,\n.c-video-player-controls__volume {\n  appearance: none;\n  background: none;\n  border: none;\n  color: #fff;\n  cursor: pointer;\n  padding: 0;\n  margin: 0 6px;\n  transition: opacity 0.3s ease;\n}\n\n.c-video-player-controls__play-pause i,\n.c-video-player-controls__fullscreen i,\n.c-video-player-controls__seek i,\n.c-video-player-controls__volume i {\n  font-size: 20px;\n}\n\n.c-video-player-controls__seek {\n  transform: translateY(2px);\n}\n\n.c-video-player-controls__play-pause:hover,\n.c-video-player-controls__fullscreen:hover,\n.c-video-player-controls__seek:hover,\n.c-video-player-controls__volume:hover {\n  opacity: 0.5;\n}\n\n.c-video-player-controls__play-pause:focus,\n.c-video-player-controls__fullscreen:focus,\n.c-video-player-controls__seek:focus,\n.c-video-player-controls__volume:focus,\n.c-video-player-controls__record:focus {\n  background-color: transparent;\n}\n\n.c-video-player-controls__time {\n  color: #fff;\n  margin-left: 20px;\n  font-size: 12px;\n  font-weight: 600;\n  letter-spacing: 1px;\n}\n\n.c-video-player-controls__playback {\n  cursor: pointer;\n  appearance: none;\n  display: inline-flex;\n  color: #fff;\n  font-size: 12px;\n  font-weight: 600;\n  line-height: 1;\n  width: auto;\n  height: auto;\n  background-color: transparent;\n  border: none;\n}\n\n.c-video-player-controls__progress-bar {\n  position: relative;\n  cursor: pointer;\n  width: 100%;\n  max-width: 100%;\n  display: flex;\n  height: 12px;\n  background-color: rgba(0, 0, 0, 0.6);\n}\n\n.c-video-player-controls__progress {\n  background-color: #039be5;\n  border-right: 4px solid #fff;\n}\n\n.c-video-player-controls__progress-time {\n  position: absolute;\n  top: -30px;\n  color: #fff;\n  background-color: rgba(0, 0, 0, 0.8);\n  padding: 0px 6px;\n  display: inline-block;\n  border-radius: 3px;\n  font-size: 14px;\n  font-weight: 600;\n  letter-spacing: 1px;\n  opacity: 0;\n  transition: opacity 0.2s ease;\n}\n\n.c-video-player-controls__progress-bar:hover\n  .c-video-player-controls__progress-time {\n  opacity: 1;\n}\n\n.c-video-player-controls__mode {\n  margin: 0 auto;\n  color: #fff;\n  font-weight: 600;\n  font-size: 12px;\n}\n\n.c-video-player-controls__mode:hover {\n  background-color: #039be5;\n}\n\n.c-video-player-loader {\n  z-index: 30;\n  position: absolute;\n  top: 0;\n  left: 0;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.6);\n}\n\n.c-video-player__help-text {\n  z-index: 30;\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  background-color: rgba(0, 0, 0, 0.6);\n  padding: 20px;\n  text-align: center;\n  color: #fff;\n  font-size: 18px;\n  border-radius: 3px;\n  font-weight: 600;\n  letter-spacing: 1px;\n  transition: all 0.3s ease;\n}\n\n.c-video-player__help-text.is-hidden {\n  opacity: 0;\n  visibility: hidden;\n}\n\n@media (max-width: 641px) {\n  .c-video-player-controls__seek,\n  .c-video-player-controls__playback,\n  .c-video-player-controls__volume {\n    display: none;\n  }\n\n  .c-video-player__help-text {\n    top: 40%;\n    left: 50%;\n    transform: translate(-50%, -40%);\n    padding: 10px;\n    max-width: 90%;\n    font-size: 14px;\n  }\n}"},"lineCount":null}},"error":null,"hash":"ca22286229804c1f2d8b51bf9f7ce4e8","cacheData":{"env":{}}}